<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>launched.my.rsync</string>
    <key>ProgramArguments</key>
    <array>
      <string>sh</string>
      <string>-c</string>
      <string>my-rsync</string>
    </array>
    <key>RootDirectory</key>
    <string>/Library/LaunchDaemons</string>
    <key>StandardErrorPath</key>
    <string>$HOME/Desktop/err.txt</string>
    <key>StandardOutPath</key>
    <string>$HOME/Desktop/out.txt</string>
    <key>StartCalendarInterval</key>
    <array>
      <dict>
        <key>Hour</key>
        <integer>19</integer>
        <key>Minute</key>
        <integer>0</integer>
      </dict>
    </array>
  </dict>
</plist>