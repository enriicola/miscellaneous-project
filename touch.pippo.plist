<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>touch.pippo</string>
    <key>ProgramArguments</key>
    <array>
      <string>touch</string>
      <string>$HOME/Desktop/pippo</string>
    </array>
    <key>StartCalendarInterval</key>
    <array>
      <dict>
        <key>Hour</key>
        <integer>15</integer>
        <key>Minute</key>
        <integer>40</integer>
      </dict>
    </array>
  </dict>
</plist>
